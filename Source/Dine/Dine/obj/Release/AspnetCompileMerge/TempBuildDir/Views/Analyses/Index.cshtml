@{
    ViewBag.Title = Dine.Utility.Constants.ToolTitlePrefix + "ADD'L CAPABILITIES";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}
<div ng-controller="snapshotController" class="master-view-content">
    <div class="submodules">
        <div class="submodules-band"></div>
        <div class="submodules-options">
            <ul>
                <li class="link_hover">
                    <div class="link_items diner_heading diner-crossdining-msehover  classMouseHover">Diner Cross Dinning Analysis</div>
                    <div class="list_content diner_subheading">
                        <div class="submodules-options-link establishment-link-compare cross-diner-analyses-msehover classMouseHover">@Html.ActionLink("Cross Diner Frequencies", "CrossDinerFrequencies", "Analyses")</div>
                    </div>
                </li>
                <li class="link_hover">
                    <div class="link_items correspondence-msehover classMouseHover">Correspondence Maps</div>
                    <div class="list_content">
                        <div class="submodules-options-link establishment-link-compare establishment-compare-analyses-msehover classMouseHover">@Html.ActionLink("Compare Establishments", "EstablishmentCompare", "Analyses")</div>
                        <div class="submodules-options-link establishment-link-deep-dive establishment-deep-dive-analyses-msehover classMouseHover">@Html.ActionLink("Establishment Deep Dive", "EstablishmentDeepDive", "Analyses")</div>
                    </div>
                </li>
            </ul>
        </div>
        <div class="submodule-border-bottom"></div>
    </div>
    <div class="red_bar"></div>
</div>

@System.Web.Optimization.Scripts.Render("~/bundle/janalyses")
@System.Web.Optimization.Styles.Render("~/bundle/sanalyses")

@*<script src="@Utils.ContentVersioned("~/Scripts/analyses.js")"> </script>
<link href="@Utils.ContentVersioned("~/Content/analyses.css")" rel="stylesheet" />*@
