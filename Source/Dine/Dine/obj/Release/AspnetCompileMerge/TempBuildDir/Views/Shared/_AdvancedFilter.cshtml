                                <div style="margin: 56px 30px 10px 20px;">
                                    <div id="slider-range"></div>
                                    <div id="slider-range-text"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Filter Panel -->
                    @*<hr />*@

                </section>
                <!--end additional filter-->
            </div> 

