                                        <span class="lft-popup-ele-next sidearrwdiv" ng-hide="panelelement.ChildCount == 0">
                                            <span class="sidearrw"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="lft-ctrl3-content" ng-switch-when="true">
                                <div class="lft-popup-col-selected-text">{{fltr.Label}}</div>
