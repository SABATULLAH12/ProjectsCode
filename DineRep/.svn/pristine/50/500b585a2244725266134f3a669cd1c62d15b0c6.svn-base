<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @System.Web.Optimization.Styles.Render("~/bundle/theme")
    @System.Web.Optimization.Scripts.Render("~/bundle/jlib")
    @System.Web.Optimization.Scripts.Render("~/bundle/d3chart")
</head>
<body>
    <div ng-app="mainApp" class="master-layout">
        <div class="master-top-header">
            <div class="master-logo"></div>
            <div class="master-settings">
                <ul class="view-actions">
                    <li>@Html.ActionLink("Logout", "Logout", "Logon")</li>
                    <li><a href="#" id="exp_data">Export</a></li>
                </ul>
            </div>
            <div class="master-all-module-links">
                <ul class="view-menu">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Chart", "Index", "Chart")</li>
                    <li>@Html.ActionLink("Snapshot", "Index", "Snapshot")</li>
                    <li>@Html.ActionLink("Table", "Index", "Table")</li>
                    <li>@Html.ActionLink("Report", "Index", "Report")</li>
                </ul>
            </div>
        </div>
        <div class="filter-info-panel">
            <div class="filter-info-panel-elements"></div>
        </div>
        <div class="master-content">
            @RenderBody()
        </div>
    </div>
</body>
</html>
